<!DOCTYPE html>
<html>
<head>
  <title>ThreeColumnLayout</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
    
    <div class="container">
        <div class="row">
            <div class="column">
                <%= yield :left_column %>
            </div>
            <div class="column">
                <%= yield :middle_column %>
            </div>
            <div class="column">
                <%= yield :right_column %>
            </div>
        </div>
    </div>

<%= yield %>

</body>
</html>
